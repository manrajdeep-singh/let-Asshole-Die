<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="android.course.project.sos"
    android:versionCode="1"
    android:versionName="1.0" >

    <uses-sdk android:minSdkVersion="17" />
    <uses-permission android:name="android.permission.READ_CONTACTS" />
    <uses-permission android:name="android.permission.CALL_PHONE" />
    <!-- uses-permission android:name="android.permission.CALL_PRIVILEGED" /-->
    <uses-permission android:name="android.permission.INTERNET" />  
    <uses-permission android:name="android.permission.ACCESS_MOCK_LOCATION" />
    <uses-permission android:name="android.permission.SEND_SMS" />
    <uses-permission android:name="android.permission.MODIFY_AUDIO_SETTINGS" />
    <uses-permission android:name="android.permission.READ_PHONE_STATE" />
    
        
    <application android:icon="@drawable/ic_launcher" android:label="@string/app_name" android:theme="@android:style/Theme.Wallpaper">
        <activity android:name="android.course.project.sos.TimerActivity" android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name="android.course.project.sos.ContactManagerActivity" android:label="@string/contact_manager"/>
        <activity android:name="android.course.project.sos.SosSettingActivity" android:label="@string/sos_setting"/>
        <activity android:name="android.course.project.sos.ContactManagerCheckboxActivity" android:label="@string/contact_manager"/>
        <activity android:name="android.course.project.sos.AlertActivity" android:label="@string/alert_manager"/>
    </application>

</manifest>